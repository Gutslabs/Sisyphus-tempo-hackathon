# Copy to web/.env.local for local development.

# xAI Grok (required for /api/chat)
GROK_API_KEY=
GROK_MODEL=grok-4-1-fast-non-reasoning

# Privy (required for wallet login UI)
NEXT_PUBLIC_PRIVY_APP_ID=

# Optional: BatchTransfer contract address (enables 1-tx CSV bulk transfers for Privy embedded wallet after approvals)
NEXT_PUBLIC_BATCH_TRANSFER_ADDRESS=

# Tempo RPC (optional)
TEMPO_RPC_URL=https://rpc.moderato.tempo.xyz

# Keeper key for /api/tempo/schedules/execute-due (optional unless you run the keeper)
KEEPER_PRIVATE_KEY=

# Optional: protect execute-due endpoint
CRON_SECRET=

# Optional: Supabase for tx history + chat sessions
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Optional: app host routing (if you use a separate dashboard subdomain)
NEXT_PUBLIC_APP_URL=
